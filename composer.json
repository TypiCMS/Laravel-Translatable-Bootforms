{
    "name": "typicms/laravel-translatable-bootforms",
    "description": "Empowers typicms/bootforms with spatie/laravel-translatable.",
    "keywords": ["laravel", "bootforms", "translatable", "i18n", "form"],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        },
        {
            "name": "Samuel De Backer",
            "email": "sdebacker@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": ">=5.8",
        "typicms/form": "^2.0.0",
        "typicms/bootforms": "^2.0.0",
        "spatie/laravel-translatable": ">=2.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "~6.0",
        "php-coveralls/php-coveralls": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "TypiCMS\\LaravelTranslatableBootForms\\TranslatableBootFormsServiceProvider"
            ]
        }
    }
}
