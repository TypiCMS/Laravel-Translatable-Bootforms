{
    "name": "typicms/laravel-translatable-bootforms",
    "description": "Empowers adamwathan/bootforms with spatie/laravel-translatable.",
    "keywords": ["laravel", "bootforms", "translatable", "i18n", "form"],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        },
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "adamwathan/bootforms": "^0.8",
        "spatie/laravel-translatable": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
